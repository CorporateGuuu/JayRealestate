{
  "name": "jay-real-estate-mobile",
  "version": "1.0.0",
  "description": "JAY Real Estate Mobile App - React Native",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace JayRealEstate.xcworkspace -scheme JayRealEstate -configuration Release -destination generic/platform=iOS -archivePath JayRealEstate.xcarchive archive",
    "clean": "react-native clean-project-auto",
    "postinstall": "cd ios && pod install"
  },
  "dependencies": {
    "react": "18.2.0",
